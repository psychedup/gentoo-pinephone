[org.gnome.desktop.background]
picture-uri='file:///usr/share/wallpapers/manjaro.jpg'

[org.gnome.desktop.wm.preferences]
button-layout='appmenu:'

[org.gnome.gedit.preferences.editor]
scheme='oblivion'

[org.gnome.desktop.interface]
enable-animations=false
icon-theme='Papirus-Dark-Maia'

[org.gnome.desktop.sound]
theme-name='librem5'

[org.gnome.settings-daemon.plugins.power]
sleep-inactive-ac-type='nothing'
sleep-inactive-battery-type='suspend'
sleep-inactive-battery-timeout=300
power-button-action='nothing'
ambient-enabled=false

[org.gnome.settings-daemon.plugins.wwan]
unlock-sim=true

[org.gnome.settings-daemon.peripherals.touchscreen]
orientation-lock=true

[org.nemo.preferences]
default-folder-viewer='compact-view'
click-policy='single'

[org.nemo.window-state]
start-with-sidebar=false

[org.gnome.eog.ui]
sidebar=false

[org.gnome.Epiphany.web]
mobile-user-agent=true
user-agent='Mozilla/5.0 (Debian device, like iPhone; X11; Linux x86_64) AppleWebKit/605.1.15 (KHTML, like Gecko) Version/13.0 Safari/605.1.15 Epiphany/605.1.15'

[sm.puri.phosh]
favorites=['sm.puri.Calls.desktop', 'sm.puri.Chatty.desktop', 'firefox.desktop', 'org.gnome.Contacts.desktop']

[org.gnome.desktop.a11y.applications]
screen-keyboard-enabled=true
